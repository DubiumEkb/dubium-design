    "lint": "eslint .",
    "preview": "vite preview",
    "version:push": "yarn publish --access=public",
    "version:patch": "yarn version patch",
    "version:minor": "yarn version minor",
    "version:major": "yarn version major"
  },
  "peerDependencies": {
    "react": ">=16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
