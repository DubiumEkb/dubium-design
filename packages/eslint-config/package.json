{
	"name": "@dubium/eslint-config",
	"private": false,
	"version": "1.0.14",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		"./base": "./config/base.js",
		"./react": "./config/react.js",
		"./jsx-a11y": "./config/jsx-a11y.js",
		"./typescript": "./config/typescript.js"
	},
	"files": [
		"config",
		"README.md",
		"LICENSE"
	],
	"description": "Flat ESLint config for Dubium projects using TypeScript, React, and accessibility best practices.",
	"keywords": [
		"eslint",
		"eslintconfig",
		"typescript",
		"react",
		"flat-config"
	],
	"homepage": "https://github.com/DubiumEkb/dubium-design/",
	"bugs": {
		"url": "https://github.com/DubiumEkb/dubium-design/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DubiumEkb/dubium-design.git"
	},
	"author": "https://github.com/DubiumEkb",
	"license": "MIT",
	"scripts": {
		"version:push": "yarn publish --access=public",
		"lint:inspect": "npx eslint --inspect-config"
	},
	"peerDependenciesMeta": {
		"@typescript-eslint/eslint-plugin": {
			"optional": true
		},
		"@typescript-eslint/parser": {
			"optional": true
		},
		"eslint-plugin-react": {
			"optional": true
		},
		"eslint-plugin-react-hooks": {
			"optional": true
		},
		"eslint-plugin-react-refresh": {
			"optional": true
		},
		"eslint-plugin-jsx-a11y": {
			"optional": true
		}
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^8.29.1",
		"@typescript-eslint/parser": "^8.29.1",
		"eslint": "^9.27.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.20",
		"typescript": "^5.8.3"
	},
	"engines": {
		"node": ">=18"
	}
}
